<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <!-- Logging -->
      <add key="serilog:using:File" value="Serilog.Sinks.File" />
      <add key="serilog:write-to:File.path" value="logs/log.txt" />
      <add key="serilog:write-to:File.fileSizeLimitBytes" />
      <add key="serilog:write-to:File.shared" value="true" />
      <!-- End Logging -->
      <!-- Tracker Api Url -->
      <add key="TrackerApi" value="https://localhost:6001"/>
      <!-- MSMQ Queue Name-->
      <add key="Queue" value="publisher"/>
    </appSettings>
    <connectionStrings>
      <!-- SQL Database connection string -->
      <add name="BackPlaneSqlConnectionString" connectionString="" />
    </connectionStrings>
</configuration>